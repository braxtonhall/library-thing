{
	"name": "Better LibraryThing",
	"version": "0.0.1",
	"manifest_version": 2,
	"description": "Makes LibraryThing BETTER",
	"permissions": ["https://*.librarything.com/*"],
	"icons": {
		"16": "img/icon16.png",
		"48": "img/icon48.png",
		"128": "img/icon128.png"
	},
	"content_scripts": [
	  {
		"matches": ["https://*.librarything.com/*"],
		"js": ["copy.js", "ensureSaved.js", "constants.js", "helpers.js"]
	  }
	]
  }